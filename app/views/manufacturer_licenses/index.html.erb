<p id="notice"><%= notice %></p>

<h1>Manufacturer Licenses</h1>

<table>
  <thead>
    <tr>
      <th>Applicant name</th>
      <th>Applicant contact</th>
      <th>Applicant address</th>
      <th>Receipt number</th>
      <th>Ein</th>
      <th>Signature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manufacturer_licenses.each do |manufacturer_license| %>
      <tr>
        <td><%= manufacturer_license.applicant_name %></td>
        <td><%= manufacturer_license.applicant_contact %></td>
        <td><%= manufacturer_license.applicant_address %></td>
        <td><%= manufacturer_license.receipt_number %></td>
        <td><%= manufacturer_license.ein %></td>
        <td><%= manufacturer_license.signature %></td>
        <td><%= link_to 'Show', manufacturer_license %></td>
        <td><%= link_to 'Edit', edit_manufacturer_license_path(manufacturer_license) %></td>
        <td><%= link_to 'Destroy', manufacturer_license, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manufacturer License', new_manufacturer_license_path %>

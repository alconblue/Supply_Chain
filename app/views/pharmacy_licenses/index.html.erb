<p id="notice"><%= notice %></p>

<h1>Pharmacy Licenses</h1>

<table>
  <thead>
    <tr>
      <th>Applicant name</th>
      <th>Applicant contact</th>
      <th>Applicant address</th>
      <th>Receipt number</th>
      <th>Ein</th>
      <th>Signature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pharmacy_licenses.each do |pharmacy_license| %>
      <tr>
        <td><%= pharmacy_license.applicant_name %></td>
        <td><%= pharmacy_license.applicant_contact %></td>
        <td><%= pharmacy_license.applicant_address %></td>
        <td><%= pharmacy_license.receipt_number %></td>
        <td><%= pharmacy_license.ein %></td>
        <td><%= pharmacy_license.signature %></td>
        <td><%= link_to 'Show', pharmacy_license %></td>
        <td><%= link_to 'Edit', edit_pharmacy_license_path(pharmacy_license) %></td>
        <td><%= link_to 'Destroy', pharmacy_license, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pharmacy License', new_pharmacy_license_path %>
